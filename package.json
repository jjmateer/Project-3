{
  "name": "project-3",
  "version": "1.0.0",
  "description": "Artificial storefront.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "lint": "eslint \"**/*.js\"",
    "test": "npm run lint",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjmateer/Project-3.git"
  },
  "author": "Melanie Mok, Travis Rodowick, John P. Mateer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jjmateer/Project-3/issues"
  },
  "homepage": "https://github.com/jjmateer/Project-3#readme",
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.7.7",
    "path": "^0.12.7",
    "react-router-dom": "^5.1.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "nodemon": "^1.18.7",
    "prettier": "1.18.2"
  }
}
